{
  "vpcNamePattern": "dev-shared-vpc-ue2-main",
  "ecsConfiguration": {
    "taskDefinition": [
      {
        "taskDefinitionName": "main",
        "containerDefinitions": [
          {
            "name": "web",
            "image": "placeholder",
            "cpu": 0,
            "portMappings": [
              {
                "containerPort": 8080,
                "hostPort": 8080,
                "protocol": "tcp",
                "appProtocol": "http"
              }
            ],
            "essential": true,
            "secrets": [
              "test-secret"
            ]
          },
          {
            "name": "trial-loader",
            "image": "placeholder",
            "cpu": 0,
            "essential": false,
            "healthCheck": {
              "command": [
                "CMD-SHELL",
                "ps || exit 1"
              ],
              "interval": 30,
              "timeout": 5,
              "retries": 3,
              "startPeriod": 60
            },
            "secrets": [
              "test-secret"
            ]
          }
        ]
      }
    ]
  }
}
{
  "ecsConfiguration": {
    "taskDefinition": {
      "containerDefinitions": [
        {
          "name": "trial-finder-v2",
          "environment": [
            {
              "name": "ENVIRONMENT",
              "value": "${ENVIRONMENT}"
            },
            {
              "name": "ACCOUNT_TYPE",
              "value": "${ACCOUNT_TYPE}"
            },
            {
              "name": "APP_VERSION",
              "value": "${APP_VERSION}"
            },
            {
              "name": "PORT",
              "value": "8080"
            },
            {
              "name": "HEALTH_CHECK_PATH",
              "value": "/health"
            }
          ]
        }
      ]
    }
  }
}